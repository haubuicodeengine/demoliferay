<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.codeengine.service.model.impl.ClazzModelImpl">
		<attributes>
			<id name="clazzId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="clazzName" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.codeengine.service.model.impl.SchoolModelImpl">
		<attributes>
			<id name="schoolId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="schoolName" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.codeengine.service.model.impl.StudentModelImpl">
		<attributes>
			<id name="userId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="name" />
			<basic name="email" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="schoolId" />
			<basic name="clazzId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.codeengine.service.model.impl.ClazzImpl" name="Clazz">
		<table name="GB_Clazz" />
		<attributes />
	</entity>
	<entity class="com.codeengine.service.model.impl.SchoolImpl" name="School">
		<table name="GB_School" />
		<attributes />
	</entity>
	<entity class="com.codeengine.service.model.impl.StudentImpl" name="Student">
		<table name="GB_Student" />
		<attributes />
	</entity>
</entity-mappings>